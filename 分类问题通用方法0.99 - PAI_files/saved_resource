.animated {
  -webkit-animation-duration: 1s;
  animation-duration: 1s;
  -webkit-animation-fill-mode: both;
  animation-fill-mode: both;
}
@-webkit-keyframes flash {
  from {
    opacity: 1;
    background-color: inherit;
  }
  25% {
    opacity: 0;
    background-color: #EF5F61;
  }
  50% {
    opacity: 1;
    background-color: #EF5F61;
  }
  75% {
    opacity: 0;
    background-color: #EF5F61;
  }
  to {
    opacity: 1;
    background-color: inherit;
  }
}
@keyframes flash {
  from {
    opacity: 1;
    background-color: inherit;
  }
  25% {
    opacity: 0;
    background-color: #EF5F61;
  }
  50% {
    opacity: 1;
    background-color: #EF5F61;
  }
  75% {
    opacity: 0;
    background-color: #EF5F61;
  }
  to {
    opacity: 1;
    background-color: inherit;
  }
}
